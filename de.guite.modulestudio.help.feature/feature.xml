<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="de.guite.modulestudio.help.feature"
      label="%featureName"
      version="1.0.0.qualifier"
      provider-name="%providerName"
      plugin="de.guite.modulestudio.branding"
      image="most_update_120.jpg"
      license-feature="de.guite.modulestudio.license"
      license-feature-version="1.0.0.qualifier">

   <description url="%descriptionUrl">
      %description
   </description>

   <copyright url="%copyrightUrl">
      %copyright
   </copyright>

   <includes
         id="org.eclipse.help"
         version="0.0.0"/>

   <requires>
      <import plugin="org.eclipse.help" version="3.6.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui.intro" version="3.4.200" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui.intro.universal" version="3.2.600" match="greaterOrEqual"/>
      <import plugin="org.eclipse.osgi" version="3.8.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui" version="3.103.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.runtime" version="3.8.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui.cheatsheets" version="3.4.200" match="greaterOrEqual"/>
      <import plugin="org.apache.lucene" version="2.9.1" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="de.guite.modulestudio.help"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="de.guite.modulestudio.help.intro"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.equinox.servletbridge.extensionbundle"
         download-size="0"
         install-size="0"
         version="0.0.0"
         fragment="true"
         unpack="false"/>

   <plugin
         id="de.guite.modulestudio.help.cheatsheets"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
